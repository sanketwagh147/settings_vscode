
{   "workbench.colorCustomizations": {
    "editorCursor.foreground": "#ffff00",
    "terminalCursor.foreground": "#ffff00"
},
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": false,
    "vim.sneak": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["leader","d"],
            "after": ["\"", "_", "d"]
        },
        {
        "before": ["<Space>","k"],
        "commands": ["editor.action.moveLinesUpAction"]
        },
        {
        "before": ["<Space>","j"],
        "commands": ["editor.action.moveLinesDownAction"]
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
    {
        "before": ["<leader>","i"],
        "after": ["i", " ", "<ESC>", "r"]
    },
    {
            "before": ["d"],
            "after": ["\"", "_", "d"]
        },  
        {
            "before": ["x"],
            "after": ["\"", "_", "x"]
        }, 
    {
            "before": ["<leader>","d"],
            "after": ["d"]
        }

    ],
    "vim.leader": "<Space>",
    "vim.handleKeys": {
        "<C-a>": true,
        "<C-f>": false,
        "<C-c>": false,
		"<C-x>": false,
		"<C-y>": true,
		"<C-v>": false,
        

    },
    "vim.operatorPendingModeKeyBindings":[ {            "before": ["L"], "after": ["$"] },
        {
            "before": ["H"],
            "after": ["^"]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                ">"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "<"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },


    ],
    "terminal.integrated.profiles.windows": {
    "PowerShell": {
    "source": "PowerShell",
    "icon": "terminal-powershell",
    "args": ["-ExecutionPolicy", "Bypass"]
    },
},
    "editor.lineNumbers": "relative",
    "editor.hover.enabled": false,
    "jupyter.askForKernelRestart": false,
    "code-runner.clearPreviousOutput": true,
    "code-runner.defaultLanguage": "python",
    "code-runner.showExecutionMessage": false,
    "code-runner.saveFileBeforeRun": true,
    "vscode-neovim.neovimExecutablePaths.win32": "C:\\tools\\neovim\\Neovim\\bin\\nvim.exe", // Controls if suggestions should be accepted with "Enter" - in addition to "Tab". Helps to avoid ambiguity between inserting new lines and accepting suggestions.
    "editor.acceptSuggestionOnEnter": "off",
    "notebook.lineNumbers": "on",
    "notebook.output.textLineLimit": 500,
    "vim.cursorStylePerMode.insert": "line",
    "vim.cursorStylePerMode.normal": "block",
    "vim.cursorStylePerMode.replace": "block",
    "vim.highlightedyank.enable": true,
    "editor.cursorSurroundingLines": 5,
    "vim.overrideCopy": true,
    "vim.useSystemClipboard": true,
    "tabnine.experimentalAutoImports": true,
    "files.associations": {
        "*.ipynb": "sql"
    },
    "workbench.sideBar.location": "right",
    "editor.minimap.enabled": false,
    "workbench.startupEditor": "none",
    "window.zoomLevel": 1,
    "vim.showMarksInGutter": true,
    "vim.smartRelativeLine": true
    ,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.editorAssociations": {
        "*.html": "default",
        "*.ipynb": "jupyter-notebook"
    },
    "window.menuBarVisibility": "toggle",
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.editor.untitled.hint": "hidden",
    "editor.autoIndent": "advanced",
    "screencastMode.verticalOffset": 5,
    "glassit.alpha": 235,
    "autoHide.panelDelay": 30,
    "autoHide.sideBarDelay": 30,
    "workbench.colorTheme": "GitHub Dark Dimmed",
    "workbench.activityBar.visible": false,
    // Comment and go down a line
    "macros": {
    "commentDown": [
        "editor.action.addCommentLine",
        "cursorDown"
    ],
    "uncommentDown": [
        "editor.action.removeCommentLine",
        "cursorDown"
    ]
                }

}
